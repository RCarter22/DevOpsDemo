<?xml version="1.0" encoding="UTF-8"?>

<EZMAXMOBILE_MAP_PROPERTIES>

	<MAP_PROVIDER name="ESRI" id="4">
		<!-- <BASE_MAP_URL key="baseMapUrl">https://services.arcgisonline.com/arcgis/rest/services/Ocean_Basemap/MapServer</BASE_MAP_URL> -->
		<BASE_MAP_URL key="baseMapUrl"/>
		<BASE_MAP_URL_OFFLINE key="offlineBaseMapUrl"/>
		<VECTOR_BASE_MAP>
			<ONLINE_USE_VECTOR key="isVectorOnline">true</ONLINE_USE_VECTOR>
			<OFFLINE_USE_VECTOR key="isVectorOffline">true</OFFLINE_USE_VECTOR>
		</VECTOR_BASE_MAP>
		<TILE_DOWNLOAD_MB_LIMIT key="tileMbLimit">500</TILE_DOWNLOAD_MB_LIMIT>
		<SPATIAL_REFERENCE key="spatialReference">4326</SPATIAL_REFERENCE>
		<!-- 
			Get the following SPATIAL_EPSG_NAME and SPATIAL_COORD_REF_PARAM strings from Spatial Reference Site:  http://spatialreference.org 
			Look for "proj4js" format
			Default Param Value for 4326 is "+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs
		!-->
		<SPATIAL_EPSG_NAME>EPSG:4326</SPATIAL_EPSG_NAME>
		<SPATIAL_COORD_REF_PARAM>+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs</SPATIAL_COORD_REF_PARAM>
		<INITIAL_POINT key="center">
			<LATITUDE key="latitude"></LATITUDE>
			<LONGITUDE key="longitude"></LONGITUDE>
		</INITIAL_POINT>
		<ZOOM>
			<!-- Set intial zoom here -->
			<INIT_LEVEL key="zoom">14</INIT_LEVEL>
			<REDO_SEARCH_LIMIT key="refreshZoomLimit">10</REDO_SEARCH_LIMIT>
			<CLUSTER_LEVELS key="clusterLevels">
				<LEVEL zoom="18" hash="12"/>
				<LEVEL zoom="17" hash="9"/>
				<LEVEL zoom="16" hash="7"/>
				<LEVEL zoom="15" hash="6"/>
				<LEVEL zoom="14" hash="5"/>
				<LEVEL zoom="13" hash="4"/>
				<LEVEL zoom="12" hash="4"/>
				<LEVEL zoom="11" hash="3"/>
				<LEVEL zoom="10" hash="3"/>
			</CLUSTER_LEVELS>
		</ZOOM>
		<MAP_LISTVIEWS>
			<ONLINE_RESULT_LIST key="onlineListView">map/list.action</ONLINE_RESULT_LIST>
			<OFFLINE_RESULT_LIST key="offlineListView">offline/map/list.htm</OFFLINE_RESULT_LIST>
			<LIST_RESULT_LIMIT key="listLimit">2000</LIST_RESULT_LIMIT>
		</MAP_LISTVIEWS>
		<MAP_SERVICES key="esriMapServers">
			<MAP_SERVICE>
				<SERVICE_ID key="esriServerId">BOSTON</SERVICE_ID>
				<SERVICE_NAME key="displayName">Boston</SERVICE_NAME>
				<SERVICE_URL key="url">http://gis.cityofboston.gov/arcgis/rest/services/CityServices/OpenData/MapServer</SERVICE_URL>
				<LAYER_DEFS key="layers">
					<LAYER objects_retrieval="true">
						<NAME key="name">WiFi Locations</NAME>
						<ID key="id">0</ID>
						<ALIAS key="alias">WiFi Locations</ALIAS>
						<FIELDS key="fields">
							<FIELD>OBJECTID</FIELD>
							<FIELD>Address</FIELD>
							<FIELD>Connect_from</FIELD>
							<FIELD>Neighborhood</FIELD>
						</FIELDS>
					</LAYER>
					<LAYER objects_retrieval="false">
						<NAME key="name">WiFi Collector</NAME>
						<ID key="id">1</ID>
						<FIELDS key="fields">
							<FIELD>OBJECTID</FIELD>
							<FIELD>Address</FIELD>
							<FIELD>Notes</FIELD>
							<FIELD>Neighborhood</FIELD>
						</FIELDS>
					</LAYER>
					<LAYER objects_retrieval="true">
						<NAME key="name">Hubway Stations</NAME>
						<ID key="id">4</ID>
						<ALIAS key="alias">Bike Stations</ALIAS>
						<FIELDS key="fields">
							<FIELD>OBJECTID</FIELD>
							<FIELD>name</FIELD>
							<FIELD>terminalName</FIELD>
							<FIELD>installDate</FIELD>
						</FIELDS>
					</LAYER>
				</LAYER_DEFS>
			</MAP_SERVICE>
		</MAP_SERVICES>
